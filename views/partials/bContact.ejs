<Section class="businessContact">
  <div class="row">
    <div class="row">
      <div class="input-field col s12">
        <p class="nameHigh">LIST OF BUSINESS CONTACTS</p>
      </div>
    </div>
    <table class="row">
      <thead>
        <tr>
          <th class="text-center">Contact Name</th>
          <th class="text-center">Contact Number</th>
          <th class="text-center">Email Address</th>
          <th class="text-center">Added By</th>
          <th class="text-center">Edit</th>
          <th class="text-center">Delete</th>
        </tr>
      </thead>
      <tbody>
        <% for (let count = 0; count < contacts.length; count++) { %>
          <tr>
            <td class="text-center"><%= contacts[count].name %></td>
            <td class="text-center"><%= contacts[count].contact %></td>
            <td class="text-center"><%= contacts[count].email %></td>
            <td class="text-center"><%= contacts[count].user %></td>
            <td class="text-center"><a class="waves-effect waves-light btn blue" href="/updateContact/<%= contacts[count]._id %>">Edit</a></td>
            <td class="text-center"><a class="waves-effect waves-light btn red" href="/delete/<%= contacts[count]._id %>">Delete</a></td>
          </tr>
        <% } %>
      </tbody>
    </table>
    <div class="row">
      <div class="col s12">
        <a class='waves-effect waves-light btn' href="/addContact" target="_self">Add Contact</a>
      </div>
    </div>
  </div>
</Section>